{
    "openapi":"3.0.3",
    "info": {
      "title":"fitcal RESTful APIs",
      "version":"1.0",
      "description": "fitcal RESTful APIs to fullfilled capstone projects",
      "contact": {
        "email": "email@gmail.com",
        "name": "name"
      }
    },
    "servers": [
      {"url":"https://core-apis-729693024400.asia-southeast2.run.app/fitcal/v1"}
    ],
    "paths": {
        "/inferences/calories":{
            "post":{
                "summary": "Doing an inference to predicts calories with in an image",
                "description": "Calling an another APIs to predicts collect the response",
                "operationId": "inferenceEventModelCalories",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Authorization",
                        "required": true,
                        "schema":{
                            "type": "string",
                            "example":"Bearer jwt_token"
                        },
                        "description": "JWT token for authentication"
                    }
                ],
                "requestBody":{
                    "required": true,
                    "content": {
                        "application/json":{
                            "schema":{
                                "type": "object",
                                "properties": {
                                    "image":{
                                        "type": "string",
                                        "format": "byte",
                                        "example":"<12 1 25 21 20>"
                                    }
                                },
                                "required": ["image"]
                            }
                        }
                    }
                },
                "responses": {
                    "201":{
                        "description": "Model predicted Successfully",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"Success"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":201
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"Model predicted Successfully"
                                        },
                                        "result":{
                                            "type":"object",
                                            "properties": {
                                                "userId":{
                                                    "type": "string",
                                                    "example":"Nayeon@gmail.com"
                                                },
                                                "InferenceId":{
                                                    "type":"string",
                                                    "example":"cwewdw-1dwda"
                                                },
                                                "result":{
                                                    "type":"string",
                                                    "example":"label"
                                                },
                                                "suggestion":{
                                                    "type": "string",
                                                    "example":"example"
                                                },
                                                "createdAt":{
                                                    "type":"string",
                                                    "example":"2024-11-12T11:06:21.790Z"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500":{
                        "description": "fail to do inference",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"failed to do inference"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":500
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"example"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "get":{
                "summary": "Collect an inference data to one user specifically",
                "description": "Calling an firestore to load inference data of user using calories model",
                "operationId": "getUserCaloriesHistories",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Authorization",
                        "required": true,
                        "schema":{
                            "type": "string",
                            "example":"Bearer jwt_token"
                        },
                        "description": "JWT token for authentication"
                    }
                ],
                "responses": {
                    "200":{
                        "description": "Data success to load",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"success to load data"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":200
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"Successfully retrieve user calories predictions histories"
                                        },
                                        "data":{
                                            "type": "array",
                                            "items":{
                                                "type": "object",
                                                "properties": {
                                                "userId":{
                                                    "type": "string",
                                                    "example":"Nayeon@gmail.com"
                                                },
                                                "inferenceId":{
                                                    "type": "string",
                                                    "example":"cwewew-12ada"
                                                },
                                                "result":{
                                                    "type": "string",
                                                    "example":"example"
                                                },
                                                "suggestion":{
                                                    "type": "string",
                                                    "example":"example"
                                                },
                                                "createdAt":{
                                                    "type": "string",
                                                    "example":"2024-11-13T15:26:08.964Z"
                                                }
                                            }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500":{
                        "description": "fail to load inference data",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"failed to load data inference"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":500
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"example"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/profile":{
