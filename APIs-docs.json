                                                },
                                                "dateOfBirth":{
                                                    "type": "string",
                                                    "example":"21 September 2024"
                                                },
                                                "gender":{
                                                    "type": "string",
                                                    "example":"male"
                                                },
                                                "emailAddress":{
                                                    "type": "string",
                                                    "example":"Nayeon@gmail.com"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500":{
                        "description": "fail to load users data",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"fail to retrieve users data"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":500
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"example"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }  
            }
        }
    }
  }