                                    "type":"object",
                                    "properties": {
                                        "status":{
                                            "type": "string",
                                            "example":"Successfully retrieve user data"
                                        },
                                        "statusCode":{
                                            "type": "number",
                                            "example":200
                                        },
                                        "message":{
                                            "type":"string",
                                            "example":"Successfully retrieve user profile data"
                                        },
                                        "data":{
                                            "type": "object",
                                            "properties": {
                                                "name":{
                                                    "type": "string",
